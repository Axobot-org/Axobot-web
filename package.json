    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@reduxjs/toolkit": "^2.2.3",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "compression": "^1.7.4",
    "discord-api-types": "^0.37.92",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "morgan": "^1.10.0",
    "mui-color-input": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-infinite-scroll-hook": "^4.1.1",
    "react-number-format": "^5.3.4",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.14.1",
    "sirv": "^2.0.4",
    "typescript": "^5.6.2",
    "typia": "^6.10.0",
    "vite": "^5.4.6",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run build && NODE_ENV=production node server",
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir build/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir build/server",
    "build-checks": "npx typia generate --input src/repository/types/checks/templates --output src/repository/types/checks/generated --project tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "2.0.1",
    "semver": "7.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.3"
  }
